generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id           Int    @id @default(autoincrement())
  name         String @db.VarChar(255)
  password     String @db.VarChar(255)
  phone_number String @db.VarChar(250) @unique
  card_number  String @db.VarChar(250)
  bank_name    String @db.VarChar(250)
}


model orders {
  id           Int    @id @default(autoincrement())
  user_id      Int    @db.Int
  order_number String @db.VarChar(250)
  status       String @db.VarChar(250)
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt
}
